{
  "name": "opencode-token-monitor",
  "version": "0.3.1",
  "description": "OpenCode plugin for monitoring token usage and ecosystem analytics",
  "author": "Ainsley0917",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ainsley0917/opencode-token-monitor.git"
  },
  "keywords": [
    "opencode",
    "plugin",
    "tokens",
    "cost",
    "monitor",
    "analytics"
  ],
  "main": "dist/plugin.js",
  "module": "plugin/index.ts",
  "types": "dist/plugin/index.d.ts",
  "type": "module",
  "private": false,
  "files": [
    "dist/plugin.js",
    "dist/plugin/index.d.ts",
    "dist/lib/",
    "pricing.example.json",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build",
    "build": "bun build ./plugin/index.ts --outfile=dist/plugin.js --target=bun --external=@opencode-ai/plugin --external=@opencode-ai/sdk && tsc --emitDeclarationOnly --declaration --project tsconfig.json --outDir dist --noEmit false"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@opencode-ai/plugin": "^1.1.51",
    "@opencode-ai/sdk": "^1.1.51"
  }
}
